{
  "name": "react-vite-panel-boilerplate",
  "private": true,
  "version": "0.5.0",
  "description": "A template for react dashboard projects",
  "type": "module",
  "homepage": "https://github.com/mohammadbrzbrz72/react-vite-panel-boilerplate#readme",
  "repository": {
    "url": "https://github.com/mohammadbrzbrz72/react-vite-panel-boilerplate",
    "type": "git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "npx eslint src --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix",
    "format": "npx prettier --write --config ./.prettierrc --ignore-unknown",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "validate-branch-name": {
    "pattern": "^(main|develop|(feat|fix|hotfix|docs|style|refactor|test|perf|ci|temp)/[A-Za-z0-9_-]+)$",
    "errorMsg": "Your branch name should follow our rules!"
  },
  "author": {
    "name": "Mohammad Barzegar",
    "email": "mohammadbrzbrz72@gmail.com",
    "url": "https://github.com/mohammadbrzbrz72/"
  },
  "license": "MIT License",
  "keywords": [
    "prettier",
    "vitejs",
    "dashboard",
    "typescript",
    "javascript"
  ],
  "dependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.19",
    "@types/node": "^20.4.1",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-icons": "^4.10.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "redux-persist": "^6.0.0",
    "reduxjs-toolkit-persist": "^7.2.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.25",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "validate-branch-name": "^1.3.0",
    "vite": "^4.4.0"
  }
}
